{
  "name": "ipa-2022-docs",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "latest"
    }
  },
  "extensions": [
    "james-yu.latex-workshop",
    "valentjn.vscode-ltex",
    "be5invis.toml",
    "yzhang.markdown-all-in-one",
    "k--kato.intellij-idea-keybindings",
